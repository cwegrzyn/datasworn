					}
				}
			},
			"optionalProperties": {
				"requirement": {
					"ref": "MarkdownString"
				},
				"count_as_impact": {
					"type": "boolean",
					"metadata": {
						"description": "If `true`, this asset counts as an impact (Starforged) or a debility (classic Ironsworn)."
					}
				},
				"controls": {
					"values": {
						"ref": "AssetControlField"
					}
				},
				"attachments": {
					"ref": "AssetAttachment"
