{
	"definitions": {
		"RegularExpression": {
			"type": "string"
		},
		"StatID": {
			"type": "string",
			"metadata": {
				"description": "A player stat (e.g. `player/stats/edge`), a player condition meter (e.g. `player/meters/health`), or an ID pointing to an asset option or asset control whose value is to be used."
			}
		},
		"Source": {
			"properties": {
				"authors": {
					"elements": {
						"type": "string"
					}
				},
				"date": {
					"type": "timestamp"
				},
				"license": {
					"ref": "URL"
				},
				"title": {
					"type": "string"
				},
				"url": {
					"ref": "URL"
				}
			},
			"optionalProperties": {
				"page": {
					"type": "uint16"
				}
			}
		},
		"ID": {
			"type": "string"
		},
		"Color": {
			"metadata": {
				"description": "A valid CSS color."
			},
			"type": "string"
		},
		"SvgImageURL": {
			"metadata": {
				"description": "A relative URL pointing to an SVG image."
			},
			"type": "string"
		},
		"WebpImageURL": {
			"metadata": {
				"description": "A relative URL pointing to a WEBP image."
			},
			"type": "string"
		},
		"URL": {
			"metadata": {
				"description": "An absolute URL pointing to a web site."
			},
			"type": "string"
		},
		"Suggestions": {
			"optionalProperties": {
				"assets": {
					"elements": {
						"ref": "AssetIDWildcard"
					}
				},
				"moves": {
					"elements": {
						"ref": "MoveID"
					}
				},
				"oracles": {
					"elements": {
						"ref": "OracleTableID"
					}
				}
			}
		},
		"AssetExtensionChoice": {
			"properties": {
				"label": {
					"ref": "Label"
				},
				"value": {
					"ref": "AssetExtension"
				}
			}
		},
		"AssetID": {
			"type": "string",
			"metadata": {
				"pattern": "^[a-z0-9_]{3,}\\/assets(\\/[a-z_]+){2}$"
			}
		},
		"AssetIDWildcard": {
			"type": "string",
			"metadata": {
				"pattern": "^([a-z_]+|\\*)\\/assets\\/([a-z_]+|\\*)\\/([a-z_]+|\\*)$"
			}
		},
		"Asset": {
			"properties": {
				"id": {
					"ref": "AssetID"
				},
				"name": {
					"ref": "Label"
				},
				"source": {
					"ref": "Source"
				},
				"abilities": {
					"elements": {
						"ref": "AssetAbility"
					}
				}
			},
			"optionalProperties": {
				"requirement": {
					"ref": "MarkdownString"
				},
				"count_as_impact": {
					"type": "boolean",
					"metadata": {
						"description": "If `true`, this asset counts as an impact (Starforged) or a debility (classic Ironsworn)."
					}
				},
				"attachments": {
					"ref": "AssetAttachment"
				},
				"controls": {
					"values": {
						"ref": "AssetControlField"
					}
				},
				"options": {
					"values": {
						"ref": "AssetOptionField"
					}
				},
				"shared": {
					"type": "boolean",
					"metadata": {
						"description": "Most assets only benefit to their owner, but certain assets (like Starforged's module and command vehicle assets) are shared amongst the player's allies, too."
					}
				}
			}
		},
		"AssetAttachment": {
			"metadata": {
				"description": "Describes which assets can be attached to this asset. The \"canonical\" example for this are Starforged's Module assets, which can be equipped by Command Vehicle assets. See p. 55 of Starforged for more info."
			},
			"properties": {
				"assets": {
					"metadata": {
						"pattern": "^([a-z_]+|\\*)\\/assets\\/([a-z_]+|\\*)\\/([a-z_]+|\\*)$"
					},
					"elements": {
						"ref": "AssetIDWildcard"
					}
				}
			},
			"optionalProperties": {
				"max": {
					"type": "uint8",
					"metadata": {
						"description": "The maximum number of attached assets. Omitted if there's no upper limit to the number of attached assets."
					}
				}
			}
		},
		"AssetAbilityID": {
			"type": "string",
			"metadata": {
				"pattern": "^[a-z0-9_]{3,}\\/assets(\\/[a-z_]+){2}\\/[0-2]$"
			}
		},
		"AssetAbility": {
			"properties": {
				"id": {
					"ref": "AssetAbilityID"
				},
				"text": {
					"ref": "MarkdownString"
				},
				"enabled": {
					"type": "boolean"
				}
			},
			"optionalProperties": {
				"name": {
					"ref": "Label"
				},
				"controls": {
					"values": {
						"ref": "AssetAbilityControlField"
					}
				},
				"options": {
					"values": {
						"ref": "AssetAbilityOptionField"
					}
				},
				"moves": {
					"values": {
						"ref": "Move"
					}
				},
				"extend_asset": {
					"ref": "AssetExtension"
				},
				"extend_moves": {
					"elements": {
						"ref": "MoveExtension"
					}
				}
			}
		},
		"AssetAbilityControlFieldID": {
			"type": "string",
			"metadata": {
				"pattern": "^[a-z0-9_]{3,}\\/assets(\\/[a-z_]+){2}\\/abilities\\/[0-2]\\/controls\\/[a-z_]+$"
			}
		},
		"AssetAbilityControlField": {
			"discriminator": "field_type",
			"mapping": {
				"checkbox": {
					"properties": {
						"id": {
							"ref": "AssetControlFieldID"
						},
						"label": {
							"ref": "Label"
						},
						"value": {
							"type": "boolean",
							"nullable": true
						}
					}
				},
				"clock": {
					"properties": {
						"id": {
							"ref": "AssetControlFieldID"
						},
						"label": {
							"ref": "Label"
						},
						"value": {
							"type": "int8"
						},
						"min": {
							"type": "int8"
						},
						"max": {
							"type": "int8"
						}
					}
				},
				"counter": {
					"properties": {
						"id": {
							"ref": "AssetAbilityControlFieldID"
						},
						"label": {
							"ref": "Label"
						},
						"value": {
							"type": "int8"
						},
						"min": {
							"type": "int8"
						},
						"max": {
							"type": "int8",
							"nullable": true
						}
					}
				}
			}
		},
		"AssetAbilityOptionFieldID": {
			"type": "string",
			"metadata": {
				"pattern": "^[a-z0-9_]{3,}\\/assets(\\/[a-z_]+){2}\\/[0-2]\\/options\\/[a-z_]+$"
			}
		},
		"AssetAbilityOptionField": {
			"discriminator": "field_type",
			"mapping": {
				"text": {
					"properties": {
						"id": {
							"ref": "AssetOptionFieldID"
						},
						"label": {
							"ref": "Label"
						}
					},
					"optionalProperties": {
						"value": {
							"type": "string"
						}
					}
				},
				"select_stat": {
					"properties": {
						"id": {
							"ref": "AssetOptionFieldID"
						},
						"label": {
							"ref": "Label"
						},
						"choices": {
							"values": {
								"properties": {
									"label": {
										"ref": "Label"
									},
									"value": {
										"ref": "PlayerStat"
									}
								},
								"optionalProperties": {
									"selected": {
										"type": "boolean"
									}
								}
							}
						}
					},
					"optionalProperties": {
						"value": {
							"ref": "PlayerStat"
						}
					}
				},
				"select_number": {
					"properties": {
						"id": {
							"ref": "AssetOptionFieldID"
						},
						"label": {
							"ref": "Label"
						},
						"choices": {
							"values": {
								"properties": {
									"label": {
										"ref": "Label"
									},
									"value": {
										"type": "int8"
									}
								},
								"optionalProperties": {
									"selected": {
										"type": "boolean"
									}
								}
							}
						}
					},
					"optionalProperties": {
						"value": {
							"type": "int8"
						}
					}
				},
				"select_asset_extension": {
					"properties": {
						"id": {
							"ref": "AssetControlFieldID"
						},
						"label": {
							"ref": "Label"
						},
						"choices": {
							"values": {
								"properties": {
									"label": {
										"ref": "Label"
									},
									"value": {
										"ref": "AssetExtension"
									}
								},
								"optionalProperties": {
									"selected": {
										"type": "boolean"
									}
								}
							}
						}
					},
					"optionalProperties": {
						"value": {
							"ref": "AssetExtension"
						}
					}
				}
			}
		},
		"AssetOptionFieldID": {
			"type": "string",
			"metadata": {
				"pattern": "^[a-z0-9_]{3,}\\/assets(\\/[a-z_]+){2}\\/options\\/[a-z_]+$"
			}
		},
		"AssetOptionField": {
			"metadata": {
				"description": "Asset options are fields that are usually only set once, typically when the player purchases the asset. The most common examples are the \"Name\" fields on companion assets. A more complex example is the choice of stats on the Devotant asset."
			},
			"discriminator": "field_type",
			"mapping": {
				"text": {
					"properties": {
						"id": {
							"ref": "AssetOptionFieldID"
						},
						"label": {
							"ref": "Label"
						}
					},
					"optionalProperties": {
						"value": {
							"type": "string"
						}
					}
				},
				"select_stat": {
					"properties": {
						"id": {
							"ref": "AssetOptionFieldID"
						},
						"label": {
							"ref": "Label"
						},
						"choices": {
							"values": {
								"properties": {
									"label": {
										"ref": "Label"
									},
									"value": {
										"ref": "PlayerStat"
									}
								},
								"optionalProperties": {
									"selected": {
										"type": "boolean"
									}
								}
							}
						}
					},
					"optionalProperties": {
						"value": {
							"ref": "PlayerStat"
						}
					}
				},
				"select_number": {
					"properties": {
						"id": {
							"ref": "AssetOptionFieldID"
						},
						"label": {
							"ref": "Label"
						},
						"choices": {
							"values": {
								"properties": {
									"label": {
										"ref": "Label"
									},
									"value": {
										"type": "int8"
									}
								},
								"optionalProperties": {
									"selected": {
										"type": "boolean"
									}
								}
							}
						}
					},
					"optionalProperties": {
						"value": {
							"type": "int8"
						}
					}
				},
				"select_asset_extension": {
					"properties": {
						"id": {
							"ref": "AssetControlFieldID"
						},
						"label": {
							"ref": "Label"
						},
						"choices": {
							"values": {
								"properties": {
									"label": {
										"ref": "Label"
									},
									"value": {
										"ref": "AssetExtension"
									}
								},
								"optionalProperties": {
									"selected": {
										"type": "boolean"
									}
								}
							}
						}
					},
					"optionalProperties": {
						"value": {
							"ref": "AssetExtension"
						}
					}
				}
			}
		},
		"AssetControlFieldID": {
			"type": "string",
			"metadata": {
				"pattern": "^[a-z0-9_]{3,}\\/assets(\\/[a-z_]+){2}\\/controls\\/[a-z_]+$"
			}
		},
		"AssetControlFieldIDWildcard": {
			"type": "string",
			"metadata": {
				"pattern": "^(\\*|[a-z0-9_]{3,})\\/assets\\/([a-z_]+|\\*)\\/([a-z_]+|\\*)\\/controls\\/[a-z_]+$"
			}
		},
		"AssetControlField": {
			"discriminator": "field_type",
			"metadata": {
				"description": "Asset controls are fields that are expected to change throughout the asset's lifespan. The most common example are the condition meters on certain assets. A more complex example is the distinct mechanical modes on Ironsworn's 'Armored'."
			},
			"mapping": {
				"condition_meter": {
					"properties": {
						"id": {
							"ref": "AssetControlFieldID"
						},
						"label": {
							"ref": "Label"
						},
						"value": {
							"type": "int8"
						},
						"min": {
							"type": "int8"
						},
						"max": {
							"type": "int8"
						}
					}
				},
				"checkbox": {
					"properties": {
						"id": {
							"ref": "AssetControlFieldID"
						},
						"label": {
							"ref": "Label"
						},
						"value": {
							"type": "boolean",
							"nullable": true
						}
					}
				},
				"select_asset_extension": {
					"properties": {
						"id": {
							"ref": "AssetControlFieldID"
						},
						"label": {
							"ref": "Label"
						},
						"choices": {
							"values": {
								"properties": {
									"label": {
										"ref": "Label"
									},
									"value": {
										"ref": "AssetExtension"
									}
								},
								"optionalProperties": {
									"selected": {
										"type": "boolean"
									}
								}
							}
						}
					},
					"optionalProperties": {
						"value": {
							"ref": "AssetExtension"
						}
					}
				}
			}
		},
		"AssetExtensionForeign": {
			"metadata": {
				"description": "Describes changes applied to an asset, usually by another asset. Unchanged properties are omitted."
			},
			"properties": {
				"extends": {
					"ref": "AssetIDWildcard"
				},
				"id": {
					"ref": "AssetAbilityControlFieldID"
				}
			},
			"optionalProperties": {
				"count_as_impact": {
					"type": "boolean"
				},
				"attachments": {
					"optionalProperties": {
						"max": {
							"type": "uint8"
						},
						"assets": {
							"elements": {
								"ref": "AssetIDWildcard"
							}
						}
					}
				},
				"controls": {
					"metadata": {
						"description": "Use the same key as the original control. Currently, only condition meters may be extended in this way."
					},
					"values": {
						"optionalProperties": {
							"max": {
								"type": "int8"
							},
							"min": {
								"type": "int8"
							}
						}
					}
				}
			}
		},
		"AssetExtension": {
			"metadata": {
				"description": "Describes changes applied to an asset by its own abilities or controls. Unchanged properties are omitted."
			},
			"optionalProperties": {
				"count_as_impact": {
					"type": "boolean"
				},
				"attachments": {
					"optionalProperties": {
						"max": {
							"type": "uint8"
						},
						"assets": {
							"elements": {
								"ref": "RegularExpression"
							}
						}
					}
				},
				"controls": {
					"metadata": {
						"description": "Use the same key as the original control. Currently, only condition meters may be extended in this way."
					},
					"values": {
						"optionalProperties": {
							"max": {
								"type": "int8"
							},
							"min": {
								"type": "int8"
							}
						}
					}
				}
			}
		},
		"DelveSiteThemeID": {
			"type": "string",
			"metadata": {
				"pattern": "^[a-z0-9_]{3,}\\/site_themes(\\/[a-z_]+){1}$"
			}
		},
		"DelveSiteDomainID": {
			"type": "string",
			"metadata": {
				"pattern": "^[a-z0-9_]{3,}\\/site_domains(\\/[a-z_]+){1}$"
			}
		},
		"DelveSiteTheme": {
			"properties": {
				"id": {
					"ref": "DelveSiteThemeID"
				},
				"name": {
					"ref": "Label"
				},
				"card_type": {
					"enum": [
						"theme"
					],
					"metadata": {
						"typescriptType": "'theme'"
					}
				},
				"source": {
					"ref": "Source"
				},
				"summary": {
					"ref": "MarkdownString"
				},
				"features": {
					"elements": {
						"ref": "FeatureOrDanger"
					},
					"metadata": {
						"typescriptType": "[\n        FeatureOrDanger & {low: 1, high: 4},\n        FeatureOrDanger & {low: 5, high: 8},\n        FeatureOrDanger & {low: 9, high: 12},\n        FeatureOrDanger & {low: 13, high: 16},\n        FeatureOrDanger & {low: 17, high: 20}\n      ]"
					}
				},
				"dangers": {
					"elements": {
						"ref": "FeatureOrDanger"
					},
					"metadata": {
						"typescriptType": "[\n        FeatureOrDanger & {low: 1, high: 5},\n        FeatureOrDanger & {low: 6, high: 10},\n        FeatureOrDanger & {low: 11, high: 12},\n        FeatureOrDanger & {low: 13, high: 14},\n        FeatureOrDanger & {low: 15, high: 16},\n        FeatureOrDanger & {low: 17, high: 18},\n        FeatureOrDanger & {low: 19, high: 20},\n        FeatureOrDanger & {low: 21, high: 22},\n        FeatureOrDanger & {low: 23, high: 24},\n        FeatureOrDanger & {low: 25, high: 26},\n        FeatureOrDanger & {low: 27, high: 28},\n        FeatureOrDanger & {low: 29, high: 30}\n      ]"
					}
				}
			},
			"optionalProperties": {
				"description": {
					"ref": "MarkdownString"
				},
				"icon": {
					"ref": "SvgImageURL"
				},
				"suggestions": {
					"ref": "Suggestions"
				}
			}
		},
		"DelveSiteDomain": {
			"properties": {
				"id": {
					"ref": "DelveSiteDomainID"
				},
				"name": {
					"ref": "Label"
				},
				"card_type": {
					"enum": [
						"domain"
					],
					"metadata": {
						"typescriptType": "'domain'"
					}
				},
				"source": {
					"ref": "Source"
				},
				"summary": {
					"ref": "MarkdownString"
				},
				"features": {
					"elements": {
						"ref": "FeatureOrDanger"
					},
					"metadata": {
						"typescriptType": "[\n          FeatureOrDanger & {low: 21, high: 43},\n          FeatureOrDanger & {low: 44, high: 56},\n          FeatureOrDanger & {low: 57, high: 64},\n          FeatureOrDanger & {low: 65, high: 68},\n          FeatureOrDanger & {low: 69, high: 72},\n          FeatureOrDanger & {low: 73, high: 76},\n          FeatureOrDanger & {low: 77, high: 80},\n          FeatureOrDanger & {low: 81, high: 84},\n          FeatureOrDanger & {low: 85, high: 88},\n          FeatureOrDanger & {low: 89, high: 98},\n          FeatureOrDanger & {low: 99, high: 99},\n          FeatureOrDanger & {low: 100, high: 100}\n        ]"
					}
				},
				"dangers": {
					"elements": {
						"ref": "FeatureOrDanger"
					},
					"metadata": {
						"typescriptType": "[\n      FeatureOrDanger & {low: 31, high: 33},\n      FeatureOrDanger & {low: 34, high: 36},\n      FeatureOrDanger & {low: 37, high: 39},\n      FeatureOrDanger & {low: 40, high: 42},\n      FeatureOrDanger & {low: 43, high: 45}\n    ]"
					}
				}
			},
			"optionalProperties": {
				"description": {
					"ref": "MarkdownString"
				},
				"icon": {
					"ref": "SvgImageURL"
				},
				"suggestions": {
					"ref": "Suggestions"
				}
			}
		},
		"FeatureOrDanger": {
			"properties": {
				"id": {
					"ref": "ID"
				},
				"low": {
					"type": "uint8"
				},
				"high": {
					"type": "uint8"
				},
				"result": {
					"ref": "MarkdownString"
				}
			},
			"optionalProperties": {
				"summary": {
					"ref": "MarkdownString"
				},
				"description": {
					"ref": "MarkdownString"
				},
				"template": {
					"ref": "OracleRollTemplate"
				},
				"icon": {
					"ref": "SvgImageURL"
				},
				"rolls": {
					"elements": {
						"ref": "OracleTableRoll"
					}
				},
				"embed_table": {
					"ref": "ID"
				},
				"suggestions": {
					"ref": "Suggestions"
				}
			}
		},
		"DelveSiteDenizen": {
			"properties": {
				"low": {
					"type": "uint8"
				},
				"high": {
					"type": "uint8"
				},
				"frequency": {
					"enum": [
						"very_common",
						"common",
						"uncommon",
						"rare",
						"unforeseen"
					]
				}
			},
			"optionalProperties": {
				"encounter": {
					"ref": "ID"
				},
				"name": {
					"ref": "Label"
				}
