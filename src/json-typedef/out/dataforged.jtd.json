{
	"definitions": {
		"RegularExpression": {
			"type": "string"
		},
		"StatID": {
			"type": "string",
			"metadata": {
				"description": "A player stat (e.g. `player/stats/edge`), a player condition meter (e.g. `player/meters/health`), or an ID pointing to an asset option or asset control whose value is to be used."
			}
		},
		"Source": {
			"properties": {
				"authors": {
					"elements": {
						"type": "string"
					}
				},
				"date": {
					"type": "timestamp"
				},
				"license": {
